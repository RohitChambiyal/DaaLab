import java.util.Scanner;
class node{
	int it=0;
	node next=null;
}

public class yesyesyes {
	node s1 = null;
	
	public void singlell(int data){
		
		
		node obj = new node();
		obj.it = data;
		obj.next = null;
		node temp = s1;
		s1 = obj;
		s1.next = temp;
	}
	public void delete(){
		s1 = s1.next;
	}
	public void display(){
		node temp = s1;
		while(temp!=null){
			System.out.println(temp.it);
			temp = temp.next;
		}
	}
	public static void main(String[] args){
		int data = 0;
		yesyesyes ob1 = new yesyesyes();
		System.out.println("Enter count of items");
		Scanner s = new Scanner(System.in);
		int n = s.nextInt();
		for(int i=0;i<n;i++){
			data=s.nextInt();
			ob1.singlell(data);
		}
		System.out.println("displaying before delete");
		ob1.display();
		ob1.delete();
		System.out.println("displaying after delete");
		ob1.display();
	}
}
